{
  "name": "apollo-proxy-cache",
  "version": "8.1.3",
  "description": "Provides an apollo-link and express (proxy) middleware setup to support a local `@cache` directive",
  "main": "build/cjs/index.js",
  "typings": "build/mjs/index.d.ts",
  "module": "build/mjs/index.js",
  "repository": "git@github.com:BowlingX/apollo-proxy-cache.git",
  "author": "David Heidrich <me@bowlingx.com>",
  "license": "MIT",
  "files": [
    "build/"
  ],
  "type": "module",
  "devDependencies": {
    "@apollo/client": "^3.10.4",
    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.8",
    "@babel/node": "^7.13.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.24.6",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.5",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@types/express": "^4.17.21",
    "@types/iltorb": "^2.3.6",
    "@types/lodash": "^4.17.4",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "apollo-utilities": "^1.3.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "http-proxy-middleware": "^2.0.6",
    "lodash": "^4.17.21",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "semantic-release": "^23.1.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn run build:main && yarn run build:cjs && sh ./esm-postbuild.sh",
    "build:main": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc --project tsconfig.types.json",
    "test": "yarn eslint src && tsc",
    "prettify": "prettier \"./src/**/*.{ts,tsx}\" --write"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./build/mjs/index.d.ts",
        "default": "./build/mjs/index.js"
      },
      "require": {
        "types": "./build/cjs/index.d.ts",
        "default": "./build/cjs/index.js"
      }
    }
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "optionalDependencies": {
    "redis": "^4.6.14"
  },
  "peerDependencies": {
    "@apollo/client": "^3",
    "@babel/polyfill": "^7",
    "apollo-utilities": "^1",
    "express": "^4",
    "graphql": ">=14",
    "http-proxy-middleware": "^2",
    "lodash": "^4"
  },
  "resolutions": {
    "browserslist": "^4.16.6",
    "hosted-git-info": "^3.0.8",
    "yargs": "^13.1.2",
    "ssri": "^8.0.1"
  },
  "dependencies": {
    "body-parser": "^1.20.2"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "yarn@4.2.2"
}
